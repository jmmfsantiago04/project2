// prisma/schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Inquiry {
  id                  Int      @id @default(autoincrement())
  fullName            String
  phoneNumber         String
  email               String
  subject             String
  content             String
  requestImageUrl     String?
  requestImagePublicId String?
}

model Submitted {
  id                  Int      @id @default(autoincrement())
  subject             String
  content             String
  requestImageUrl     String?
  requestImagePublicId String?
}

